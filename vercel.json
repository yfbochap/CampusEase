{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/index.html",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      }
    ]
  }
  